<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core">
  <SelectDialog
    title="Personnel Group"
    class="sapUiPopupWithPadding"
    items="{/HT501tSet}"
    search="_handleValueHelpSearchPersonnelGroup"
    confirm="_handleValueHelpClosePersonnelGroup"
    cancel="_handleValueHelpClosePersonnelGroup">
    <StandardListItem
      iconDensityAware="false"
      iconInset="false"
      title="{Persg}"
      description="{Ptext}" />
  </SelectDialog>
</core:FragmentDefinition>